{
  "name": "gov-au-transitions-beta",
  "version": "1.0.0",
  "private": true,
  "description": "BETA version of transitions aged care app",
  "engines": {
    "node": "5.10.0",
    "npm": "3.5.0"
  },
  "scripts": {
    "build:test": "webpack --config webpack.config.js",
    "build:production": "NODE_ENV=production webpack --config webpack.config.js",
    "build:development": "webpack -w --config webpack.config.js",
    "test": "NODE_ENV=test ava",
    "test:coverage": "NODE_ENV=test nyc --reporter=lcov --reporter=text --reporter=html ava",
    "test:check-coverage": "nyc check-coverage --statements 100 --branches 100 --functions 100 --lines 100",
    "lint": "eslint app __tests__"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/AusDTO/govau-transitions-beta.git"
  },
  "author": "transitions team",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/AusDTO/govau-transitions-beta/issues"
  },
  "homepage": "https://github.com/AusDTO/govau-transitions-beta#readme",
  "cacheDirectories": [
    "node_modules",
    "client/node_modules"
  ],
  "dependencies": {
    "es5-shim": "^4.5.7",
    "font-awesome": "^4.6.3",
    "gov-au-ui-kit": "git+https://github.com/nathf/gov-au-ui-kit.git#feature/node-install-issues",
    "react": "^15.0.0",
    "react-dom": "^15.0.0",
    "react-on-rails": "6.0.5",
    "react-redux": "^4.4.1",
    "redux": "^3.3.1",
    "redux-thunk": "^2.0.1",
    "webfontloader": "^1.6.26"
  },
  "devDependencies": {
    "autoprefixer": "^6.4.0",
    "ava": "^0.16.0",
    "babel": "^6.5.2",
    "babel-cli": "^6.6.5",
    "babel-core": "^6.7.4",
    "babel-eslint": "^6.1.2",
    "babel-loader": "^6.2.4",
    "babel-polyfill": "^6.7.4",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-0": "^6.5.0",
    "babel-register": "^6.14.0",
    "babel-runtime": "^6.11.6",
    "css-loader": "^0.24.0",
    "enzyme": "^2.4.1",
    "eslint": "^3.4.0",
    "eslint-config-rackt": "^1.1.1",
    "eslint-plugin-ava": "^3.0.0",
    "eslint-plugin-react": "^6.2.0",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.9.0",
    "imports-loader": "^0.6.5",
    "jsdom": "^9.4.2",
    "node-sass": "^3.8.0",
    "nyc": "^8.1.0",
    "postcss-loader": "^0.11.0",
    "react-addons-test-utils": "^15.3.1",
    "sass-loader": "^4.0.0",
    "url-loader": "^0.5.7",
    "webpack": "^1.13.2",
    "webpack-closure-compiler": "^2.0.2"
  },
  "ava": {
    "babel": "inherit",
    "require": [
      "./__tests__/helpers/setup-test-env.js"
    ]
  }
}
